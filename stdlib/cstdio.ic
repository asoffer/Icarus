// This module provides an interface to functions in the <stdio.h> C-header file.

#{export} File   ::= 'opaque
#{export} fopen  ::= foreign("fopen", (*nat8, *nat8) -> *File)
#{export} fclose ::= foreign("fclose", *File -> int32)
#{export} fgets  ::= foreign("fgets", (*nat8, int32, *File) -> *nat8)
#{export} fgetc  ::= foreign("fgetc", (*File) -> int32)
#{export} puts   ::= foreign("puts", (*nat8) -> int32)
#{export} eof    ::= 4294967295 as int32
