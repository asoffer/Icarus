// Types aliases for compatibility with C.

// TODO: This is a platform-dependent value and should be made configurable.
#{export} int       ::= i32
#{export} long      ::= i64
#{export} long_long ::= i64

#{export} unsigned_int       ::= u32
#{export} unsigned_long      ::= u64
#{export} unsigned_long_long ::= u64

#{export} size_t ::= u64

#{export} float ::= f32
#{export} double ::= f64
