-- ::= import "examples/lib/core.ic"
io ::= import "examples/lib/io.ic"

i := 0
while (i < 100) do {
  if (i % 15 == 0) then {
    io.Print("FizzBuzz\n")
  } else if (i % 3 == 0) then {
    io.Print("Fizz\n")
  } else if (i % 5 == 0) then {
    io.Print("Buzz\n")
  } else {
    io.Print(i)
    io.Print("\n")
  }
  i += 1
}
io.Print("\n")
