-- ::= import "examples/lib/core.ic"

main ::= () -> () {
  i := 0
  while (i < 100) do {
    if (i % 15 == 0) then {
      print "FizzBuzz\n"
    } else { if (i % 3 == 0) then {
      print "Fizz\n"
    } else { if (i % 5 == 0) then {
      print "Buzz\n"
    } else {
      print i, "\n"
    } } }
    i += 1
  }
  print "\n"
}
