for (0, 11) do [num: i64] {
  io.Print("fib(", num, ") = ", fibonacci(num), "\n")
}

// Functions can appear after they're referenced.
fibonacci ::= (n: i64) -> i64 {
  if (n < 2) { return n }
  return fibonacci(n - 1) + fibonacci(n - 2)
}

// Imports can appear after they're referenced, too.
