-- ::= import "examples/lib/core.ic"
io ::= import "examples/lib/io.ic"

num := 0
upper ::= 2_000_000
n := 2
while (n <= upper) do {
  found_div := false
  test := 2
  while (test * test <= n) do {
    if (n % test == 0) then {
      found_div = true
      test = n // Force break
    }
    test += 1
  }

  if (!found_div) then { num += 1 }
  n += 1
}

io.Print("There are ")
io.Print(num)
io.Print(" prime numbers less than or equal to ")
io.Print(upper)
io.Print("\n")
