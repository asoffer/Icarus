-- ::= import "core.ic"
io ::= import "io.ic"

num: i64 = 0
upper ::= 1_000
for (2, upper) do [n: i64] {
  found_div := false
  test: i64 = 2
  while (test * test <= n) {
    if (n % test == 0) {
      found_div = true
      test = n // Force break
    }
    test += 1
  }

  if (not found_div) { num += 1 }
  n += 1
}

io.Print("There are ", num, " prime numbers less than or equal to ", upper as i64, "\n")
