-- ::= import "examples/lib/core.ic"

main ::= () -> () {
  num := 0
  upper ::= 1_000
  n := 2
  while (n <= upper) {
    found_div := false
    test := 2
    while (test * test <= n) {
      if (n % test == 0) {
        found_div = true
        test = n // Force break
      }
      test += 1
    }

    if (!found_div) { num += 1 }
    n += 1
  }
  print "There are ", num, " prime numbers less than or equal to ", upper, "\n"
}
